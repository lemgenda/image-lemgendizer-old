{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": "vite",
    "functions": {
        "api/screenshot.js": {
            "maxDuration": 30,
            "memory": 2048
        },
        "api/health.js": {
            "maxDuration": 10,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}