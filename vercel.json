{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/screenshot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/screenshot",
      "dest": "/api/screenshot"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/screenshot.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}